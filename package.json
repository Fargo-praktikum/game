    "main": "server/index.ts",
    "scripts": {
        "test": "jest",
        "build": "npm-run-all lint lint-styles test webpack-prod",
        "build-dev": "npm-run-all lint lint-styles webpack-dev",
        "lint": "eslint .",
        "lint-styles": "stylelint \"src/**/*.scss\"",
        "lint-fix": "eslint . --fix",
        "webpack-prod": "set HTTP_PROTOCOL=http&& set NODE_ENV=production&& webpack",
        "webpack-dev": "webpack --mode development",
        "prepare": "husky install",
        "docker:compose:build": "docker compose build",
        "docker:compose:up": "docker compose up",
        "start:compile": "set HTTP_PROTOCOL=https&& set NODE_ENV=development&& webpack --watch",
        "start-new:container": "npm-run-all docker:compose:build docker:compose:up --print-label",
        "start": "npm-run-all --print-label --parallel start:*",
        "stop:container": "docker compose stop"
    },
